{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DbConnection": "<From Key Vault>"
  },
  "InternaleSusFarmServiceAddress": "<From Key Vault>",
  "InternaleSusFarmServiceSettings": {
    "CacheExpirationInMinutes": 15
  },
  "StorageConfiguration": {
    "AccountName": "<From Key Vault>",
    "AccountKey": "<From Key Vault>",
    "ProxyKey": "<From Key Vault>",
    "FileShare": "<From Key Vault>"
  },
  "PasswordHashPepper": "<From Key Vault>",
  "JWT": {
    "ValidAudience": "<From Key Vault>",
    "ValidIssuer": "<From Key Vault>",
    "Secret": "<From Key Vault>",
    "TokenValidityInMinutes": 5,
    "RefreshTokenValidityInDays": 7
  },
  "EmailConfiguration": {
    "From": "<From Key Vault>",
    "SmtpServer": "<From Key Vault>",
    "Port": 587,
    "Username": "<From Key Vault>",
    "Password": "<From Key Vault>"
  }
}
